<script type="text/javascript">
	$(function(){
		var cishu=0;
		//搜索
		$('#searchbtn').click(function(){
			var number1=$('#search').val();
//			alert(number1);
			$(this).attr('href','#'+number1);
		});
		//填号码弹窗
		//关闭
		$('.telclose').click(function(){
			$('.box-tel').fadeOut(500);
		});
		//确定按钮
		$('.telbtn').click(function(){
			var reg=/^[1]\d{10}/;
			var tel=$('#tel').val();
			if(reg.test(tel)){
				alert('填写的号码：'+tel);
			}else{
				alert('请填写正确的号码！');
			}
			
		});
		//判断号码是否填过
		var thistel=1;//判断条件
		if(thistel==1){
			$('.box-tel').fadeIn(500);
		}else{
			$('.box-tel').fadeOut(500);
		}
		if(cishu<3){
			$('.selectbtn').click(function(){
				$(this).attr('src','img/xuanzebtn1.png');
				var renshu=$(this).siblings('p').children('.renshu').html();
				renshu=parseInt(renshu)+1;
				cishu++;
				$(this).attr('title',cishu);
				if(cishu<=3){
					$(this).siblings('p').children('.renshu').html(renshu);
					$(this).siblings('.xiaoguo').css({'top':'-0.44rem','visibility':'visible'});
				    setTimeout(function(){
				    	$('.xiaoguo').css({'visibility':'hidden'});
				    },500);
				     setTimeout(function(){
				    	$('.xiaoguo').css({'top':'0.2rem'});
				    },1000);
				}else if(cishu=3){
					$('.tanchuang').fadeIn(500);
					setTimeout(function(){
						$('.tanchuang').fadeOut(500);
					},1000);
				}
				console.log(cishu);
				
			});
		}else{
			$('.tanchuang').fadeIn(500);
					setTimeout(function(){
						$('.tanchuang').fadeOut(500);
					},1000);
		}
		$('.fanhui').click(function(){
			$('.dabox').scrollTop(0);
			
		});
	})
</script>
	</body>
</html>
